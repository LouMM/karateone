    "@types/leaflet": "^1.5.12",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/react-leaflet": "^2.5.1",
    "@types/react-router-dom": "^5.1.3",
    "@types/webpack": "^4.41.10",
    "css-loader": "^3.4.2",
    "file-loader": "^6.0.0",

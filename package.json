{
  "name": "karateone",
  "version": "0.0.1",
  "description": "Karate One",
  "main": "index.js",
  "scripts": {
    "build:win": "NODE_ENV=prod && ts-node webpack.config.ts",    
    "build": "./node_modules/.bin/webpack --progress --color --display-error-details --mode production",
    "build:dev": "./node_modules/.bin/webpack --progress --color --display-error-details --mode development",
    "serve:win": "set ENV=prod&& ts-node server.production.tsx",
    "serve": "ENV=prod ts-node server.production.tsx",
    "servejs": "ENV=prod node ./dist/server.bundle.js",  
    "start": "ENV=prod ts-node server.production.tsx",  
    "express:dev:win" :"./node_modules/.bin/ts-node server.production.tsx",
    "start:dev": "./node_modules/.bin/webpack-dev-server"
  },
